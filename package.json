{
  "name": "xsrt",
  "version": "0.1.1",
  "description": "",
  "main": "src/viewer/index.tsx",
  "scripts": {
    "start:viewer": "webpack-dev-server --config-name viewer-dev",
    "start:backend": "npm run build:backend && concurrently -p \"[{name}]\" \"node ./dist/backend/api/api-server.js\" \"node ./dist/backend/decorators/decorator-server.js\"",
    "build:backend": "tsc -p tsconfig.api.json",
    "build:viewer": "webpack --config-name viewer-prod",
    "build:tools": "webpack --mode development --config-name bootstrap-scripts",
    "build:extension": "webpack --watch --mode development --config-name compile-extension",
    "build:test": "tsc -p tsconfig.spec.json",
    "build:test:watch": "tsc -p tsconfig.spec.json --watch",
    "test": "npm run build:test && jasmine --config=jasmine.json",
    "test:watch": "mkdir -p dist/spec && rm -r dist/spec && concurrently -p \"[{name}]\" --restart-tries 3 \"npm:build:test:watch\" \"npm:test:watch:build-artifacts\"",
    "test:watch:build-artifacts": "nodemon -w dist/spec/* --delay 1000ms ./node_modules/.bin/jasmine --config=jasmine.json"
  },
  "author": "Ryan Kadri",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "0.0.75",
    "@types/classnames": "^2.2.6",
    "@types/clean-webpack-plugin": "^0.1.2",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/jasmine": "^3.3.5",
    "@types/mongoose": "^5.3.1",
    "@types/node": "^10.12.10",
    "@types/pako": "^1.0.0",
    "@types/puppeteer": "^1.10.0",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-test-renderer": "^16.0.3",
    "@types/shortid": "0.0.29",
    "@types/ua-parser-js": "^0.7.32",
    "@types/webpack": "^4.4.17",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^4.6.0",
    "html-webpack-plugin": "^3.2.0",
    "jasmine": "^3.3.1",
    "nodemon": "^1.18.9",
    "nyc": "^13.1.0",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^4.3.12",
    "react-test-renderer": "^16.6.3",
    "ts-loader": "^5.3.0",
    "tslint": "^5.12.1",
    "typescript": "^3.1.6",
    "webpack": "^4.24.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": ">=3.1.11",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@material-ui/core": "^3.6.0",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.18.0",
