{
  "name": "xsrt",
  "version": "0.1.1",
  "description": "A Cross-Session Screen Recording tool (XSRT) also known as exert",
  "repository": {
    "type": "git",
    "url": "https://github.com/RyanKadri/xsrt"
  },
  "main": "src/viewer/index.tsx",
  "scripts": {
    "start:viewer": "webpack-dev-server --config-name viewer-dev",
    "start:api": "npm run build:backend && npm run run:api",
    "run:api": "node ./dist/backend/api-service.bundle.js",
    "start:decorator": "npm run build:backend && npm run run:viewer",
    "run:viewer": "node ./dist/backend/decorator-service.bundle.js",
    "build:backend": "webpack --config-name compile-backend",
    "build:viewer": "webpack --config-name viewer-prod",
    "build:tools": "webpack --mode development --config-name bootstrap-scripts",
    "build:extension": "webpack --watch --mode development --config-name compile-extension",
    "build:docker": "webpack --config webpack-docker.config.js --mode development",
    "build:docker:watch": "npm run build:docker -- --watch",
    "test": "node --require ./spec/support/init.js ./node_modules/.bin/jasmine --config=jasmine.json",
    "test:coverage": "nyc ./node_modules/.bin/jasmine --config=jasmine.json",
    "test:watch": "nodemon -w src -e ts,tsx --exec \"node --require ./spec/support/init.js ./node_modules/.bin/jasmine --config=jasmine.json\"",
    "lint": "tslint --project tsconfig.json"
  },
  "author": "Ryan Kadri",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "0.0.75",
    "@types/classnames": "^2.2.6",
    "@types/clean-webpack-plugin": "^0.1.2",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/express": "^4.16.0",
    "@types/jasmine": "^3.3.5",
    "@types/mongoose": "^5.3.1",
    "@types/node": "^10.12.10",
    "@types/pako": "^1.0.0",
    "@types/puppeteer": "^1.10.0",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-test-renderer": "^16.0.3",
    "@types/shortid": "0.0.29",
    "@types/ua-parser-js": "^0.7.32",
    "@types/webpack": "^4.4.17",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^4.6.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "html-webpack-plugin": "^3.2.0",
    "jasmine": "^3.3.1",
    "node-loader": "^0.6.0",
    "nodemon": "^1.18.9",
    "nyc": "^13.1.0",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^4.3.12",
    "react-test-renderer": "^16.6.3",
    "source-map-support": "^0.5.10",
    "ts-loader": "^5.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "typescript": "^3.1.6",
    "webpack": "^4.24.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": ">=3.1.11",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@material-ui/core": "^3.6.0",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "buffer": "^5.2.1",
    "classnames": "^2.2.6",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "inversify": "^5.0.1",
    "mongoose": "^5.4.6",
    "pako": "^1.0.6",
    "puppeteer": "^1.10.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router-dom": "^4.3.1",
    "reflect-metadata": "^0.1.12",
    "shortid": "^2.2.14",
    "tsconfig-paths": "^3.7.0",
    "tslib": "^1.9.3",
    "ua-parser-js": "^0.7.19"
  }
}
